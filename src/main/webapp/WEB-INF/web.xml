<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
    <display-name>Servlet</display-name>
    <servlet>
        <servlet-name>servletcalculator</servlet-name>
        <servlet-class>com.natal.calculator.Calculator</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>hits</servlet-name>
        <servlet-class>com.natal.calculator.Hits</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>init</servlet-name>
        <servlet-class>com.natal.calculator.Init</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servletcalculator</servlet-name>
        <url-pattern>/calculator</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>hits</servlet-name>
        <url-pattern>/hits</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>init</servlet-name>
        <url-pattern>/init</url-pattern>
    </servlet-mapping>
</web-app>